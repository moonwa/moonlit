<?xml version="1.0" encoding="utf-8" ?>
<models xmlns="http://hizhanzhang.com/moonlit/mvc/maint/models"  namespace="Moonlit.Mvc.Maintenance.Models"  defaultResourceType="Moonlit.Mvc.Maintenance.Properties.MaintCultureTextResources">
  <import>Moonlit.Mvc.Maintenance.Properties</import>
  <import>Moonlit.Mvc.Maintenance.SelectListItemsProviders</import>
  <model name="AdminUserListModel" resourceName="AdminUser">
    <properties>
      <property name="Keyword" type="string">
        <display name="Keyword"/>
        <field width="6">
          <textbox />
        </field>

      </property>
      <property name="UserName" type="string">
        <field width="6">
          <textbox />
        </field>
      </property>
      <property name="IsEnabled" type="bool?">
        <field width="6">
          <checkbox />
        </field>
      </property>
    </properties>
    <methods>
      <administrationSimpleListTemplate >
      </administrationSimpleListTemplate>
    </methods>
  </model>
  <model name="AdminUserCreateModel" resourceName="AdminUser">
    <mapping default="enabled" mappingType="User" />
    <properties>
      <property name="UserName" type="string">
        <mapping />
        <field width="6" />
      </property>
      <property name="LoginName" type="string">
        <mapping />
        <field width="6" />
      </property>
      <property name="Password" type="string">
        <mapping stub="true" />
        <field width="6">
          <passwordbox/>
        </field>
      </property>
      <property name="Gender" type="Gender?">
        <mapping />
        <field width="6" />
      </property>
      <property name="DateOfBirth" type="DateTime?">
        <mapping />
        <field width="6" />
      </property>
      <property name="IsSuper" type="bool" enabled="false">
        <mapping />
        <field width="6" />
      </property>
      <property name="Roles" type="int[]" >
        <mapping stub="true"/>
        <field width="6">
            <multiselectlist provider="Role" />
        </field>
      </property>
      <property name="IsEnabled" type="bool">
        <mapping />
        <field width="6">
        </field>
      </property>
    </properties>
    <methods>
      <administrationSimpleEditTemplate   title="AdminUserEdit"  description="AdminUserEditDescription" panelTitle="AdminUserInfo" />
      <entityMappingFrom target="User" /> 
      <entityMappingTo target="User" /> 
    </methods>
  </model>
  <model name="AdminUserEditModel" resourceName="AdminUser">
    <mapping default="enabled" mappingType="User" />
    <properties>
      <property name="UserName" type="string">
        <mapping />
        <field width="6" />
      </property>
      <property name="LoginName" type="string" enabled="false">
        <mapping />
        <field width="6" />
      </property>
      <property name="Password" type="string">
        <mapping stub="true"/>
        <field width="6">
          <passwordbox/>
        </field>
      </property>
      <property name="Gender" type="Gender?">
        <mapping />
        <field width="6" />
      </property>
      <property name="DateOfBirth" type="DateTime?">
        <mapping />
        <field width="6" />
      </property>
      <property name="IsSuper" type="bool" enabled="false">
        <mapping />
        <field width="6" />
      </property>
      <property name="Roles" type="int[]" >
        <mapping stub="true"/>
        <field width="6">
            <multiselectlist provider="Role" />
        </field>
      </property>
      <property name="IsEnabled" type="bool">
        <mapping/>
        <field width="6">
        </field>
      </property>
    </properties>
    <methods>
      <administrationSimpleEditTemplate  title="AdminUserEdit"  description="AdminUserEditDescription" panelTitle="AdminUserInfo" />
      <entityMappingFrom target="User" /> 
      <entityMappingTo target="User" /> 
    </methods>
  </model>
</models>