@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>

    <meta http-equiv="Content-type" content="text/html; charset=utf-8">
    <title>@ViewBag.Title</title>

    <link rel="stylesheet" href="//src.cdn.hizhanzhang.com/libs/dojo/1.11.1/dijit/themes/claro/claro.css">
    <style type="text/css">
        html, body {
            width: 100%;
            height: 100%;
            margin: 0;
            overflow: hidden;
        }

        #borderContainer {
            width: 100%;
            height: 100%;
        }
    </style>
    <script>dojoConfig = {parseOnLoad: true}</script>
    <script src='//src.cdn.hizhanzhang.com/libs/dojo/1.11.1/dojo/dojo.js'></script>
</head>
<body class="claro">
@RenderBody()
</body>
</html>